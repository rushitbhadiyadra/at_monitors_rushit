				"body": {
					"mode": "raw",
					"raw": "{\n  \"name\": \"TA-41\",\n  \"description\": \"\",\n  \"trigger\": \"contact_created\",\n  \"trigger_frequency\": \"once\",\n  \"trigger_sources\": [\n    \"ui\",\n    \"api\"\n  ],\n  \"btn_save\": \"\",\n  \"criteria\": [\n    {\n        \"condition\": \"and\",\n        \"group\": \"\",\n        \"field_id\": {{list_field_country}},\n        \"field_name\": \"country\",\n        \"type\": \"string\",\n        \"position\": 0,\n        \"changed\": \"\",\n        \"operator\": \"=\",\n        \"case_sensitive\": \"1\",\n        \"operand\": [\n            \"mkl\"\n        ],\n        \"left_parenthesis\": 0,\n        \"right_parenthesis\": 0\n    }\n  ],\n  \"actions\": [\n    {\n      \"name\": \"TA-41 Webhook Action\",\n      \"action_number\": 1,\n      \"type\": \"webhook\",\n      \"data\": {\n        \"http_method\": \"post\",\n        \"url\": \"https://automated_tests-l2.ongage.net/webhook_testing/{{env}}-ta41\",\n        \"selected_fields\": [\n          \"email\"\n        ]\n      },\n      \"active\": true,\n      \"position\": 0\n    }\n  ]\n}"
				},
