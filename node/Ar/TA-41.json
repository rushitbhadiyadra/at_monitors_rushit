				{
					"listen": "test",
					"script": {
						"exec": [
							"if(pm.response.code != '200' ){",
							"    postman.setNextRequest('Create contact');",
							"}else{",
							"    pm.globals.set(\"automation_rule_id\", pm.response.json().payload.id);",
