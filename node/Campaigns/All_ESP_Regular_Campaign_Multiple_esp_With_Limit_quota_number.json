		{
			"name": "Create campaign and run",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var jsonData = pm.response.json();",
							"pm.globals.set(\"campaign_id_multiple_esp_with_limit\", jsonData.payload.id);",
							"pm.globals.unset(\"time_30\");"
