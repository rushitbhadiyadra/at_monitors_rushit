							"        if(jsonData.payload.id !== undefined && jsonData.payload.id !== null){\r",
							"        tableHtml +=\"<tr><td style='width: 20%;'>Campaign Id</td><td style='width: 50%;'>\"+jsonData.payload.id+\"</td></tr>\"\r",
