					"listen": "test",
					"script": {
						"exec": [
							"if (pm.response.code === 200) {",
							"   var jsonData = pm.response.json();",
							"   pm.test(\"Test Transactional Send List Level Status\", function () {",
							"      pm.expect(jsonData.payload.failed_emails['rc_suppression_list1@hoohem.com']).to.eql(\"Suppressed\");",
							"   });",
							"   pm.test(\"Test Transactional Send Globel Level Status\", function () {",
							"      pm.expect(jsonData.payload.failed_emails['rc_suppression_list2@hoohem.com']).to.eql(\"Suppressed\");",
							"   });",
							"   pm.test(\"Test Transactional Send Campaign Level\", function () {",
							"      pm.expect(jsonData.payload.failed_emails['rc_suppression_list3@hoohem.com']).to.eql(\"Suppressed\");",
