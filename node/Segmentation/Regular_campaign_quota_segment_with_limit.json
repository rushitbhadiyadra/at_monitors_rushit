							"var jsonData = pm.response.json()",
							"count = 0",
							"if(jsonData.payload.length > 0){",
							"    for (var segment of jsonData.payload) {",
							"        if (segment.name === \"Regular campaign quota segment with limit-\" + pm.globals.get(\"connectionId\")){",
							"            count = 1",
							"            pm.globals.set(\"segment_id_regular_campaign_quota_segment_limit\", segment.id);      ",
